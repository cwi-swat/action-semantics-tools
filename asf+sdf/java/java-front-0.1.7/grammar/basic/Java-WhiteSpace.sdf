module Java-WhiteSpace
exports
  lexical syntax
    WhiteSpace     -> LAYOUT

    [\13]          -> LineTerminator
    [\n][\13]?     -> LineTerminator

    [\ \t\12]      -> WhiteSpace
    LineTerminator -> WhiteSpace

  context-free restrictions
    LAYOUT? -/- [\ \t\n\13]

