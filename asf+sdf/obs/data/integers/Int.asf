~~ -- cut off subtraction -?
   [140]  gt(n1, n2) = true
          ===
          n1 -? n2 = n1 >- n2

   [141]  gt(n1, n2) != true
          ===
          n1 -? n2 = 0

~~ -- subtraction of nat-conurals

   [142]  gt(n1, n2) = true 
          ===
          n1 - n2 = n1 >- n2

   [143]  gt(n1, n2) != true 
          ===
          n1 - n2 = -(n2 >- n1)

