~~ -- addition,  subtraction, and multiplication of integers
   [148]  +n = n

   [149]  n1 + -n2 = n1 - n2
   [150]  -n1 + n2 = n2 - n1
   [151]  n1 + n2 = n  === -n1 + -n2 = -n

   [152]  n1 - -n2 = n1 + n2
   [153]  n1 + n2 = n === -n1 - n2 = -n
   [154]  -n1 - -n2 = n2 - n1

   [155]  n1 * n2 = n  === n1 * -n2 = -n
   [156]  n1 * n2 = n === -n1 * n2 = -n
   [157]  -n1 * -n2 = n1 * n2

~~ -- relational operators
   [158]  gt(n1, n2) = true === n1 > n2 = true
   [159]  gt(n1, n2) != true === n1 > n2 = false
   [160]  n1 > -n2 = true
   [161]  -n1 > n2 = false
   [162]  -n1 > -n2 = n2 > n1

   [163]  i1 != i2 === i1 >= i2 = i1 > i2
